{
  "name": "budgeter",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "16.16.x",
    "npm": "8.x"
  },
  "scripts": {
    "start:dev:fe": "cd client && npm run dev",
    "start:dev:be": "cd server && npm run dev",
    "start:dev": "concurrently npm:start:dev:fe npm:start:dev:be",
    "npm-install-client": "cd client && npm install",
    "npm-install-server": "cd server && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && node server.js",
    "delete": "rm -rf client/node_modules && rm -rf server/node_modules && rm -rf client/dist",
    "postinstall": "npm run npm-install-client && npm run npm-install-server"
  },
  "dependencies": {
    "concurrently": "^7.2.2"
  }
}
